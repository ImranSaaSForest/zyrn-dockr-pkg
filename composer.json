{
    "name": "zyrn/dockr-pkg",
    "description": "A Composer package to set up Docker containers with PHP 8.1, MySQL 8, and Nginx.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Zyrn\\DockrPkg\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zyrn\\DockrPkg\\DockrServiceProvider"
            ]
        }
    }
}
